<%= render 'header'%>

<%= form_tag(url_for(:action => :save_tour, :controller => :content)) do |f| %>
    City Name: <%= text_field_tag :name %><br>
    Country Name: <%= select_tag :country_id, options_for_select(Country.get_all_countries_map) %><br>
    Places and attractions(seperate by |): <%= text_area_tag :places_and_attractions %><br>
    <%= submit_tag 'Submit' %>

<% end %>

